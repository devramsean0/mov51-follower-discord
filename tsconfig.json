{
	"extends": "@sapphire/ts-config",
	"compilerOptions": {
		"rootDir": "src",
		"outDir": "dist",
		"tsBuildInfoFile": "dist/.tsbuildinfo",
        "module": "commonjs",
        "moduleResolution": "Node", 
        "allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
	},
	"include": ["src"]
}